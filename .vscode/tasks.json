{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build all files",
            "command": "/usr/sbin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}/*.c",
                "${fileDirname}/../header/*.h",
                "-o",
                "${fileDirname}/../hashmap"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
    ],
    "version": "2.0.0"
}